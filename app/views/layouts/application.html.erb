<!DOCTYPE html>
<html>
<head>
  <title>Vineline</title>
  <%= include_gon %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <script type="text/javascript" src="//use.typekit.net/upk1hbm.js"></script>
	<script type="text/javascript">try{Typekit.load();}catch(e){}</script>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'layouts/header' %>
<% if flash[:notice] %>
	<div class="alert">
		<%= flash[:notice] %>
	</div>
<% end %>
<%= yield %>
<%= render 'layouts/footer' %>
</body>
</html>